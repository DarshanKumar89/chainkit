schema UniswapV3Swap:
  version: 2
  description: "Uniswap V3 Swap event — emitted on every swap through a V3 pool"
  supersedes: UniswapV2Swap
  deprecated: false
  chains: [ethereum, arbitrum, polygon, base, optimism]
  address: null
  event: Swap
  fingerprint: "0xc42079f94a6350d7e6235f29174924f928cc2ac818eb64fed8004e115fbcca67"
  fields:
    sender:       { type: address, indexed: true,  description: "Address that initiated the swap" }
    recipient:    { type: address, indexed: true,  description: "Address that received the output tokens" }
    amount0:      { type: int256,  indexed: false, description: "Change in token0 (negative = token0 sold)" }
    amount1:      { type: int256,  indexed: false, description: "Change in token1 (negative = token1 sold)" }
    sqrtPriceX96: { type: uint160, indexed: false, description: "Pool sqrt price after the swap (Q64.96)" }
    liquidity:    { type: uint128, indexed: false, description: "Pool liquidity after the swap" }
    tick:         { type: int24,   indexed: false, description: "Current tick after the swap" }
  meta:
    protocol: uniswap-v3
    category: dex
    verified: true
    trust_level: maintainer_verified
---
schema UniswapV3Mint:
