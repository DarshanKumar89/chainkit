  fields:
    sender:      { type: address, indexed: false }
    owner:       { type: address, indexed: true  }
    tickLower:   { type: int24,   indexed: true  }
    tickUpper:   { type: int24,   indexed: true  }
    amount:      { type: uint128, indexed: false }
    amount0:     { type: uint256, indexed: false }
    amount1:     { type: uint256, indexed: false }
  meta:
    protocol: uniswap-v3
    category: dex
    verified: true
    trust_level: maintainer_verified
