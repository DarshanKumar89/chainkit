  "files": [
    "index.d.ts",
    "index.js",
    "chaincodec.*.node"
  ],
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "prepublishOnly": "napi build --platform --release --strip",
    "version": "napi version && git add -A"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0",
    "typescript": "^5.3.0",
    "jest": "^29.0.0"
